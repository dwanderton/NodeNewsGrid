    <script type="text/javascript">
    function drawFaveLike() {
  // Some raw data (not necessarily accurate)
  var data = google.visualization.arrayToDataTable([
    ['Month',   'Faves', 'Views'],
    ['2004/05',    165,  400],
    ['2005/06',    135,      1120],
    ['2006/07',    500,      1167],
    ['2007/08',    350,      1110],
    ['2008/09',    136,      691]
  ]);

  // Create and draw the visualization.
  var ac = new google.visualization.AreaChart(document.getElementById('faveLike'));
  ac.draw(data, {
    isStacked: true,
    width: 400,
    height: 300,
    vAxis: {gridlines: { color: 'transparent' }},
    legend: {position: 'none'}
  });
}

      var gauge;
    var gaugeData;
    var gaugeOptions;
    function drawGauge() {
    gaugeData = google.visualization.arrayToDataTable([
    ['eye icon', 'favicon'],
    [120, 80]
    ]);

    gauge = new google.visualization.Gauge(document.getElementById('gauge'));
    gaugeOptions = {
    min: 0,
    max: 280,
    yellowFrom: 30,
    yellowTo: 130,
    redFrom: 0,
    redTo: 30,
    greenFrom:130,
    greenTo: 280,
    minorTicks: 5
    };
    gauge.draw(gaugeData, gaugeOptions);
    }
google.setOnLoadCallback(drawGauge); 
google.setOnLoadCallback(drawFaveLike);  
    </script>
